{
  "rules": {
    "no-any": true,
    "no-inferrable-types": true,
    "no-reference": true,
    "no-var-requires": true,
    "curly": true,
    "no-arg": true,
    "no-eval": true,
    "no-construct": true,
    "no-duplicate-variable": true,
    "no-debugger": true,
    "no-invalid-this": true,
    "no-unused-expression": true,
    "triple-equals": true,
    "no-var-keyword": true,
    "radix": true,
    "no-require-imports": true,
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "one-line": [
      true,
      "check-catch",
      "check-finally",
      "check-else",
      "check-open-brace",
      "check-whitespace"
    ],
    "no-consecutive-blank-lines": true,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type",
      "check-typecast"
    ],
    "eofline": true,
    "indent": [
      true,
      "spaces"
    ],
    "max-line-length": [
      true,
      120
    ],
    "no-angle-bracket-type-assertion": true,
    "new-parens": true,
    "class-name": true,
    "variable-name": [
      true,
      "ban-keywords"
    ],
    "semicolon": [true, "always"],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ]
  }
}